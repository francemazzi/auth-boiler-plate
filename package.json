    "start": "node dist/infrastructure/http/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "test": "jest"
  },
  "keywords": [
    "express",
    "typescript",
    "prisma",
    "postgresql",
    "authentication",
    "boilerplate",
    "starter",
    "template"
  ],
  "author": "Your Name",
  "license": "MIT",
