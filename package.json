{
  "name": "create-express-auth",
  "version": "1.0.11",
  "description": "CLI to create an Express boilerplate with authentication, TypeScript, Prisma and PostgreSQL",
  "main": "src/infrastructure/http/server.ts",
  "bin": {
    "create-express-auth": "./bin/create-express-auth.js"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/infrastructure/http/server.ts",
