    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@eslint/config-array": "^2.1.1",
    "@eslint/object-schema": "^2.0.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
